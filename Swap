ackage InterviewCoding;

public class Swap {
    public static void main(String[] args){
        int a=20,b=30,t;
        t=a;            //swap use of third variable
        a=b;
        b=t;
        System.out.println("a "+a);
        System.out.println("b "+b);

        /* int a=10,b=20;
        a=a+b;    //without third variable swap
        b=a-b;
        a=a-b;
        System.out.println("a"+a);
        System.out.println("b"+b);*/

    }
}
